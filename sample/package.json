{
  "name": "sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:build:bundle": "react-native bundle  --entry-file='index.js' --bundle-output='android/rnsample/src/main/assets/index.android.bundle' --dev=false --platform='android'  --assets-dest='android/rnsample/src/main/res'",
    "android:fonts:clear": "rm -r android/rnsample/src/main/assets/fonts",
    "android:fonts:copy": "cp -a android/app/src/main/assets/fonts android/rnsample/src/main/assets/fonts",
    "android:fonts:update": "yarn clear-android-fonts && yarn copy-android-fonts",
    "android:generate:aar": "yarn update-android-fonts && cd android && ./gradlew :rnsample:assembleRelease",
    "ios": "react-native run-ios",
    "ios:build:bundle": "react-native bundle --entry-file='index.js' --bundle-output='./ios/rn-sample-ios-lib/Resources/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios/rn-sample-ios-lib/Resources/'",
    "ios:fonts:clear": "rm -r ios/rn-sample-ios-lib/Resources/assets/fonts",
    "ios:fonts:copy": "cp -a node_modules/@naturacosmeticos/natds-icons/dist/fonts ios/rn-sample-ios-lib/Resources/assets/",
    "ios:fonts:update": "yarn ios:fonts:clear && yarn ios:fonts:copy",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@naturacosmeticos/natds-icons": "^0.21.39",
    "@naturacosmeticos/natds-rn": "^3.0.0",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "0.63.3",
    "styled-components": "5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
